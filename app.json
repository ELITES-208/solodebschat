{
  "expo": {
    "name": "Solo Chat",
    "slug": "solo-chat",
    "privacy": "unlisted",
    "description": "An application for communication within SOLODEBS ENTERPRISE",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elites.solochat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon1.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.elites.solochat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
